# Docker Environment Variables
# Copy this file to .env and fill in your actual values

# JWT Secret (minimum 32 characters) - Generate a secure random string
JWT_SECRET="your_jwt_secret_here_min_32_characters"

# Google Gemini AI API Key - Get from https://makersuite.google.com/app/apikey
GEMINI_API_KEY="your_gemini_api_key_here"

# Razorpay Payment Gateway - Get from https://razorpay.com/
RAZORPAY_KEY_ID="your_razorpay_key_id"
RAZORPAY_KEY_SECRET="your_razorpay_key_secret"

# Gmail SMTP Configuration - Use Gmail App Password
GMAIL_USER="your_email@gmail.com"
GMAIL_APP_PASSWORD="your_gmail_app_password"

# Judge0 API (RapidAPI) - Get from https://rapidapi.com/judge0-official/api/judge0-ce
RAPIDAPI_KEY="your_rapidapi_key_here"
